<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="false">

    <bean id="parentBeanFactory" class="jmine.tec.utils.spring.CachingClassPathXmlApplicationContext">
        <constructor-arg>
            <list>
                <value>core-beans.xml</value>
                <value>transaction-beans.xml</value>
                <value>jmine-tec-report.xml</value>
                <value>application-config-beans.xml</value>
            </list>
        </constructor-arg>
    </bean>
</beans>