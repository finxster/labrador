<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Loans.
    -->
    <changeSet id="20170117201043-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="bone_id"
                                 baseTableName="loans"
                                 constraintName="fk_loans_bone_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bone"/>

        <addForeignKeyConstraint baseColumnNames="taker_id"
                                 baseTableName="loans"
                                 constraintName="fk_loans_taker_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
